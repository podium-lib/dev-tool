		"npm-run-all2": "6.2.6",
		"patch-package": "8.0.0",
		"prettier": "3.6.2",
		"semantic-release": "23.1.1",
		"semantic-release-chrome": "3.2.0",
		"semantic-release-firefox-add-on": "0.2.8",
		"typescript": "5.7.2"
